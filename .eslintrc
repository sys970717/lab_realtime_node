{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "node": true,
    "mocha": true
  },
    "rules": {
    "max-length": 0, // 0으로 설정하면 에러 검출 X
    "no-restricted-globals": 0,
    "no-param-reassign": 0,
    "no-else-return": ["error", { "allowElseIf": true }],
    "prefer-promise-reject-errors": 0,
    "consistent-return": 0,
    "no-unused-vars": 1,
    "no-restricted-syntax": 0,
    "no-continue": 0,
    "no-await-in-loop": 0,
    "no-throw-literal": 0,
    "camelcase": 0,
    "strict": 0
  },
}